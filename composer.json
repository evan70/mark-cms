{
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.14",
        "slim/psr7": "^1.7",
        "php-di/php-di": "^7.0",
        "illuminate/database": "^10.0",
        "illuminate/collections": "^10.0",
        "illuminate/view": "^10.48",
        "illuminate/filesystem": "^10.48",
        "vlucas/phpdotenv": "^5.6",
        "middlewares/trailing-slash": "^2.1",
        "slim/csrf": "^1.5",
        "monolog/monolog": "^3.9",
        "bryanjhv/slim-session": "^4.1",
        "symfony/console": "^6.0",
        "nesbot/carbon": "^2.73",
        "illuminate/support": "^10.48",
        "erusev/parsedown": "^1.7",
        "league/commonmark": "^2.6",
        "symfony/yaml": "^7.2",
        "openai-php/client": "^0.10.3",
        "symfony/http-client": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "pnpm install"
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
